{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "lib": ["dom", "es6"],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "declaration": true,
    "outDir": "./lib"
  },
  "include": [
    "src/**/*"
  ]
}
